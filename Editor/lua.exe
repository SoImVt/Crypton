<?xml version="1.0"?>
<SyntaxDefinition name = "Lua"
                  extensions = ".lua">
  <Properties>
    <Property name="LineComment"
              value="--"/>
  </Properties>

  <Digits name = "Digits" bold = "false" italic = "false" color = "#F99157"/>

  <RuleSets>
    <RuleSet ignorecase="false">
      <Delimiters>~@$%^&amp;*()+=|\[]{};"'&lt;&gt; ,#:	</Delimiters>

      <Span name = "BlockComment" bold = "false" italic = "false" color = "#999999" stopateol = "false">
        <Begin>--[[</Begin>
        <End>]]</End>
      </Span>
      <Span name = "BlockCommentTwo" bold = "false" italic = "false" color = "#999999" stopateol = "false">
        <Begin>[=[</Begin>
        <End>]=]</End>
      </Span>

      <Span name = "LineComment" bold = "false" italic = "false" color = "#999999" stopateol = "true">
        <Begin>--</Begin>
      </Span>

      <Span name="FirstString" color="#99CC99" stopateol="true" bold="false" italic="false">
        <Begin>"</Begin>
        <End>"</End>
      </Span>
	  
	  <Span name="SecondString" color="#99CC99" stopateol="true" bold="false" italic="false">
        <Begin>'</Begin>
        <End>'</End>
      </Span>

      <KeyWords name="Punctuation" bold="false" italic="false" color="#66CCCC">
	  <Key word = "#" />
	  	  <Key word = "%" />
	  <Key word = "^" />
	  <Key word = "*" />
	  <Key word = "+" />
	  <Key word = "-" />
	  <Key word = "/" />
	  <Key word = ".." />
	  </KeyWords>
    <KeyWords name="KeyWords" bold="false" italic="false" color="#CC99CC">
<Key word="while"/>
<Key word="or"/>
<Key word="then"/>
<Key word="and"/>
<Key word="do"/>
<Key word="end"/>
<Key word="elseif"/>
<Key word="if"/>
<Key word="for"/>
<Key word="in"/>
<Key word="foreach"/>
<Key word="return"/>
<Key word="not"/>
<Key word="local"/>
<Key word="gcinfo"/>
<Key word="function"/>
    </KeyWords>
        <KeyWords name="Tables" bold="false" italic="false" color="#F99157">
<Key word="string"/>
<Key word="os"/>
<Key word="io"/>
<Key word="math"/>
<Key word="debug"/>
